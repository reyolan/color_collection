<div class="h-[calc(100%-64px)] bg-slate-600">
  <h1 class="text-5xl font-bold text-white text-center">All Colors</h1>
  <% @colors.each do |color|%>
    <div class="flex flex-col justify-center">
      <div class="h-16 w-16" style="background-color:#<%= color.hex%>"></div>
      <div><%= color.name%></div>
    </div>
  <%end%>
</div>
